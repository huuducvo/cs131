#1 Find 10 pickup location with highest avarage total amount in August, 2019
 1133  awk -F, '{sum[$8]+=$17; num[$8]++} END {for(x in sum){print x, sum[x]/ num[x]}}' 2019-08.csv | sort -k2nr | head -10 > a3.txt

#2 Find the pickup and dropoff location with the highest total amount in August, 2019
 1135  awk -F, '{sum[$8, $9]+=$17; num[$8, $9]++} END {for(x in sum){split(x,sep,SUBSEP); print sep[1], sep[2], sum[sep[1], sep[2]]/ num[sep[1], sep[2]]}}' 2019-08.csv | sort -k2nr | head -1 >> a3.txt

#3 Graph the correlation between the number of passengers and the tip amount in August 2019
 1025  awk -F, 'BEGIN{OFS=","} {sum[$4]+=$14; num[$4]++} END {for(x in sum){print x, sum[x]/ num[x]}}' 2019-08.csv | sort -k2nr | head -10 > a3q3.dat

